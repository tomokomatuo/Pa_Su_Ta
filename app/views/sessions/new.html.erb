<%= link_to "新規登録", new_user_path %>
<h1>ログイン</h1>
<%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>
  <div classs="file_filed">
    <%= f.label :email %>
    <%= f.email_field :email %>
  </div>
  <div classs="file_filed">
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>
  <%= f.submit "ログイン" %>
<% end %>
